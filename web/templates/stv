; A simple transitive verb template
label=Transitive verb phrase
[ LTOP: h1
INDEX: e2 [ e SORT: SEMSORT @TR-VERB1@ ]
RELS: <
  [ "#DET1#"
  LBL: h3
  ARG0: x4 [ x SORT: SEMSORT @NOUN1@ ]
  RSTR: h5
  BODY: h6 ]
  [ "#NOUN1#"
  LBL: h7
  ARG0: x4 ]
  [ "#TR-VERB1#"
  LBL: h13
  ARG0: e2
  ARG1: x4
  ARG2: x8 [ x SORT: SEMSORT @NOUN2@ ] ]
  [ "#DET2#"
  LBL: h9
  ARG0: x8
  RSTR: h10
  BODY: h11 ]
  [ "#NOUN2#"
  LBL: h12
  ARG0: x8 ] >
HCONS: < h5 qeq h7 h10 qeq h12 > ] 
